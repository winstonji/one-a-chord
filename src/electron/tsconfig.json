{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "ES5",
		"outDir": "dist",
		"moduleResolution": "node",
		"module": "commonJS",
		"sourceMap": true,
		"removeComments": true,
		"noImplicitAny": false,
		"baseUrl": "./src",
		"paths": {
			"*": ["*", "./services", "../global/*"]
		}
	},
	"include": ["./**/*.ts", "../global/**/*.ts"],
	"exclude": ["./dist"]
}
